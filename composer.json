{
    "name": "tlr/menu",
    "description": "Take some of the stress and boilerplate out of building menus (or indeed any list, because that's basically what a menu is) With support for laravel",
    "keywords": ["laravel","menu","list"],
    "license": "MIT",
    "authors": [
        {
            "name": "Stef Horner",
            "email": "stef@tedslittlerobot.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "~5.0",
        "patchwork/utf8": "1.1.*",
        "laravelcollective/html": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Tlr\\Menu\\": "src/classes"
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "dev-master"
    }
}
